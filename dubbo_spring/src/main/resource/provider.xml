<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd">
    <dubbo:application name="dm-dubbo" >
        <!--qos是否关闭-->
        <dubbo:parameter key="qos.enable" value="false"></dubbo:parameter>
        <!--qos运维命令是否允许远程访问-->
        <!--<dubbo:parameter key="qos.accept.foreign.ip" value="false"></dubbo:parameter>-->
        <!--qos运维命令端口-->
        <!--<dubbo:parameter key="qos.port" value="22222"></dubbo:parameter>-->
    </dubbo:application>
    <dubbo:registry address="zookeeper://122.51.157.42:2181"/>
    <dubbo:config-center timeout="50000"></dubbo:config-center>
    <dubbo:protocol port="-1"></dubbo:protocol>
    <dubbo:provider/>
    <dubbo:service interface="com.dm.UserService" ref="userService">
    </dubbo:service>
    <bean id="userService" class="com.dm.UserServiceImpl"></bean>
</beans>