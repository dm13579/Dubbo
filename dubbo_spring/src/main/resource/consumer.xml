<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd">
    <dubbo:application name="dm-dubbo">
        <dubbo:parameter key="qos.enable" value="false"></dubbo:parameter>
    </dubbo:application>
    <dubbo:registry address="zookeeper://122.51.157.42:2181"/>
    <dubbo:config-center timeout="50000"></dubbo:config-center>
    <dubbo:consumer/>
    <dubbo:reference id="userService" interface="com.dm.UserService" loadbalance="roundrobin">
    </dubbo:reference>

</beans>